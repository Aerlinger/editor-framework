{
  "name": "editor-console",
  "version": "0.0.1",
  "description": "A simple console editor",
  "author": "Firebox Technology",
  "main": "main.js",
  "menus": {
    "Developer/Editor Console": {
      "message": "editor-console:open"
    }
  },
  "panels": {
    "panel": {
      "view": "panel/console.html",
      "type": "dockable",
      "title": "Editor Console",
      "icon": "panel/icon.png",
      "height": 300,
      "min-height": 200,
      "shortcuts": {
        "command+k": "clear"
      },
      "messages": [
        "console:log",
        "console:success",
        "console:failed",
        "console:info",
        "console:warn",
        "console:error"
      ]
    }
  }
}
