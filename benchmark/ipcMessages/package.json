{
  "name": "benchmark-ipc-messages",
  "version": "0.0.1",
  "description": "Test ipc messages performance",
  "author": "Firebox Technology",
  "main": "main.js",
  "menus": {
    "Benchmark/IpcMessages": {
      "message": "benchmark-ipc-messages:open"
    }
  },
  "panels": {
    "panel1": {
      "frame": "panel/panel1.html",
      "type": "dockable",
      "title": "Benchmark - Ipc Messages 1",
      "width": 600,
      "height": 400,
      "min-width": 600,
      "min-height": 400,
      "messages": [
        "benchmark:ipc-messages-renderer-to-from-renderer",
        "benchmark:ipc-messages-main-to-renderer",
        "benchmark:end-ipc-messages-main-from-renderer",
        "benchmark:end-ipc-messages-renderer-to-renderer"

      ]
    },
    "panel2": {
      "frame": "panel/panel2.html",
      "type": "dockable",
      "title": "Benchmark - Ipc Messages 2",
      "width": 600,
      "height": 400,
      "min-width": 600,
      "min-height": 400,
      "messages": [
        "benchmark:ipc-messages-renderer-to-from-renderer",
        
        "benchmark:begin-ipc-messages-renderer-to-renderer",
        "benchmark:ipc-messages-renderer-to-renderer"
      ]
    }
  }
}
