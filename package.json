{
    "name": "editor-framework",
    "version": "0.3.14",
    "description": "A simple framework for making panel based applications.",
    "homepage": "http://fireball-x.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/fireball-x/editor-framework.git"
    },
    "author": "Firebox Technology",
    "license": "MIT License",
    "bugs": {
        "url": "https://github.com/fireball-x/editor-framework/issues"
    },
    "scripts": {
        "preinstall": "npm install npmconf@2.1.2 && node ./utils/pre-install-npm.js",
        "postinstall": "gulp post-install-npm",
        "install": "gulp bootstrap && bower install && gulp npm-rebuild && gulp update",
        "test": "node utils/run-tests.js",
        "api": "firedoc build ./ --theme notab"
    },
    "dependencies": {
        "async": "1.2.1",
        "chai": "2.3.0",
        "chalk": "1.0.0",
        "chokidar": "1.0.2",
        "commander": "2.8.1",
        "del": "1.2.0",
        "fire-fs": "0.2.1",
        "fire-path": "0.1.7",
        "fire-url": "0.1.6",
        "globby": "2.0.0",
        "gulp": "3.8.11",
        "gulp-babel": "5.1.0",
        "gulp-less": "3.0.3",
        "gulp-sequence": "0.3.2",
        "gulp-stylus": "2.0.3",
        "diff": "1.4.0",
        "lodash": "3.9.3",
        "mocha": "2.2.5",
        "semver": "4.3.6",
        "sinon": "1.15.0",
        "winston": "1.0.0"
    },
    "devDependencies": {
        "check-dependencies": "0.9.2",
        "firedoc": "1.9.0",
        "gulp-changed": "1.2.1",
        "gulp-jshint": "1.11.0",
        "gulp-watch": "4.2.4",
        "jshint-stylish": "2.0.0",
        "npmconf": "2.1.2"
    },
    "sharedPackages": [
        "benchmark",
        "package-examples"
    ],
    "builtins": [
        "console",
        "ipc-debugger",
        "package-manager",
        "tester",
        "ui-kit"
    ],
    "nativeModules": [
        "chokidar"
    ],
    "electronVersion": "0.30.4",
    "main": "demo.js"
}
