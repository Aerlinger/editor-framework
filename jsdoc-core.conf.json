{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": [
      "./core/",
      "./share/"
    ],
    "exclude": [
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false
    },
    "applicationName": "Editor (Core-Level)",
    "footer": "Made with ♥ by <a href='http://fireball-x.com'>Fireball Team</a>",
    "copyright": "Fireball Copyright © 2013-2015.",
    "disqus": "",
    "googleAnalytics": "",
    "openGraph": {
      "title": "",
      "type": "website",
      "image": "",
      "site_name": "",
      "url": ""
    },
    "meta": {
      "title": "",
      "description": "",
      "keyword": ""
    },
    "linenums": true
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "private": false,
    "lenient": true,
    "destination": "./api/core",
    "template": "./node_modules/jaguarjs-jsdoc"
  }
}
